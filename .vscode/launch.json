{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "clashbot_flutter",
            "request": "launch",
            "type": "dart",
            "args": [
                "--web-port",
                "4200"
            ],
        },
        {
            "name": "Golden",
            "request": "launch",
            "type": "dart",
            "codeLens": {
                "for": [
                    "run-test",
                    "run-test-file"
                ]
            },
            "args": [
                "--update-goldens"
            ]
        },
        {
            "name": "clashbot_flutter_storybook",
            "request": "launch",
            "type": "dart",
            "args": [
                "-t",
                "main.widgetbook.dart"
            ],
        },
        {
            "name": "clashbot_flutter (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "clashbot_flutter (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}